                                    <div class="post_commentbox"><span><i class="fa fa-calendar"></i>{{date_created}}</span></div>
                                </li>
                                {{/each}}
                            </ul>
                        </div>
                    </div>
                </div>
                <div class="col-lg-6 col-md-6 col-sm-6">
                    <div class="content_middle_rightbar">
                        <div class="single_category wow fadeInDown" style="margin-top:-20px">
