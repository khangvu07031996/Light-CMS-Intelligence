<div class="content_top">
    <div class="row">
        <div class="col-lg-6 col-md-6 col-sm6">
            <div class="latest_slider">
                <div class="slick_slider">
                    {{#each arrLastArticle}}
                    <div class="single_iteam">
                        <img src="http://localhost:3000/{{this}}" alt="" style="height:430px">
                    </div>
                    {{/each}}
                    <h2><a class="slider_tittle" href="../detail/{{lastArticlesdata._id}}">{{lastArticlesdata.headline}}</a></h2>
                </div>
            </div>
            <div class="post_commentbox" style="margin-bottom:0px"><span><i class="fa fa-calendar"></i>{{lastArticlesdata.date_created}}</span> <a href="#"><i class="fa fa-tags"></i>{{lastArticlesdata.section}}</a></div>

        </div>
        <div class="col-lg-6 col-md-6 col-sm6">
            <div class="content_top_right">
                <ul class="featured_nav wow fadeInDown">
                    {{#each Article}}

                    <li><img src="http://localhost:3000/{{images}}" alt="">
                        <div class="title_caption"><a href="../detail/{{_id}}">{{headline}}</a></div>
                        <div class="post_commentbox"><span><i class="fa fa-calendar"></i>{{date_created}}</span> <a style="display : inline;color: #798992; font-size: 11px;" href="#"><i class="fa fa-tags"></i>{{section}}</a></div>

                    </li>

                    {{/each}}
                </ul>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12">
            <div class="col-lg-3 col-md-3 col-sm-3">
                <div class="content_middle_leftbar">
                    <div class="single_category wow fadeInDown">
                        <h2> <span class="bold_line"><span></span></span> <span class="solid_line"></span> <a href="#" class="title_text">Technology</a></h2>
                        <ul class="catg1_nav">
                            {{#each tech}}
                            <li>
                                <div class="catgimg_container"> <a href="../detail/{{_id}}" class="catg1_img"><img alt="" src="http://localhost:3000/{{images}}"></a></div>
                                <h3 class="post_titile"><a href="../detail/{{_id}}">{{headline}}</a></h3>
                                <div class="post_commentbox"><span><i class="fa fa-calendar"></i>{{date_created}}</span></div>
                            </li>
                            {{/each}}
                        </ul>
                    </div>
                </div>
            </div>
            <div class="col-lg-6 col-md-6 col-sm-6">
                <div class="content_middle_middle">
                    <h2 style="margin-top:-12px"> <span class="bold_line" style="margin-left:3px; width:99%"><span></span></span> <span class="solid_line"></span><a href="#" class="title_text">Sport</a></h2>
                    <div class="slick_slider2">
                        {{#each sport}}
