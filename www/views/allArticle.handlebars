                            <h2><a href="../detail/{{_id}}">{{headline}}</a></h2>
                            <p>
                                {{premble}}
                            </p>
                            <div class="post_commentbox" style="margin-bottom:18px"><span><i class="fa fa-calendar"></i>{{date_created}}</span> </div>
                        </div>
                        {{/each}}
                    </div>
                </div>
                <div class="col-lg-6 col-md-6 col-sm-6">
                    <div class="content_middle_rightbar">
                        <div class="single_category wow fadeInDown" style="margin-top:-20px">
                            <h2> <span class="bold_line"><span></span></span> <span class="solid_line"></span> <a href="#" class="title_text">World</a></h2>
