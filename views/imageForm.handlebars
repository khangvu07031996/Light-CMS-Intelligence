
        <!--main content start-->
 
                <div class="row">
                    <div class="col-lg-12">
                        <h3 class="page-header"><i class="fa fa-user"></i>imageForm</h3>
                        <ol class="breadcrumb">
                            <li><i class="fa fa-home"></i><a href="index.html">Home</a></li>
                            <li><i class="fa fa-book"></i>Forms</li>
                            <li><i class="fa fa-users"></i>images</li>
                        </ol>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-12">
                        <section class="panel">
                            <header class="panel-heading">
                                Advanced Table
                            </header>
                                 <div >
                                    <a class="btn btn-primary" href="/image/add" data-toggle="modal">
                                        Add image
                                    </a>
                                 </div>
                            <table class="table table-striped table-advance table-hover">
                                <tbody>
                                    <tr>
                                        <th><i class="icon_image"></i> Image </th>
                                        <th><i class="icon_profile"></i> UserCreate</th>
                                       
                                        <th><i class="icon_calendar"></i> Heading</th>
                                        <th><i class="icon_calendar"></i> Description</th>
                                        <th><i class="icon_calendar"></i> Photographer</th>
                                        
                                        <!--<th><i class="icon_profile"></i> User_update</th>-->
                                        <th><i class="icon_cogs"></i> Action</th>
                                         
                                    </tr>
                                        {{#each images}}
                                    <tr>
                                        <td><img src="{{media}}/{{medialist.articlePreview}}" style="width : 100px; height : 100px"></td>
                                        <td>{{usercreate}}</td>
                                       
                                        <td>{{heading}}</td>
                                        <td>{{description}}</td>
                                        <td>{{photographer}}</td>
                                        
                                        
                                        <td>
                                            <div class="btn-group">
                                               
                                                <a class="btn btn-success" href="/image/{{_id}}"><i class="icon_pencil"></i></a>
                                                <a class="btn btn-danger" href="/image/delete/{{id}}" onclick="return confirm('Are you sure you want to delete this item?');"><i class="icon_close_alt2"></i></a>
                                            </div>
                                        </td>
                                    </tr>
                                    {{/each}}
                                </tbody>
                            </table>
                        </section>
                    </div>
                </div>
    