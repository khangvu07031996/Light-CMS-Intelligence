
                <div class="row">
                    <div class="col-lg-12">
                        <h3 class="page-header"><i class="fa fa-book"></i>KeywordForm</h3>
                        <ol class="breadcrumb">
                            <li><i class="fa fa-home"></i><a href="index.html">Home</a></li>
                            <li><i class="fa fa-book"></i>Forms</li>
                            <li><i class="fa fa-th-list"></i>KeywordForm</li>
                        </ol>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-12">
                        <section class="panel">
                            <header class="panel-heading">
                                Advanced Table
                            </header>
                                <div >
                                    <a class="btn btn-primary" href="/Keyword/add" data-toggle="modal">
                                        AddKeyword
                                    </a>
                                 </div>
                            <table class="table table-striped table-advance table-hover">
                                <tbody>
                                    <tr>
                                        <th><i class="icon_profile"></i> Name</th>
                                        <th><i class="icon_calendar"></i> Date Created</th>
                                        <th><i class="icon_calendar"></i> Date Updated</th>
                                        <th><i class="icon_cogs"></i> Action</th>
                                    </tr>
                                    {{#each keyword}}
                                    <tr>
                                        <td>{{name}}</td>
                                        <td>{{date date_created}}</td>
                                        <td>{{date date_update}}</td>
                                        
                                        <td>
                                            <div class="btn-group">
                                                
                                                <a class="btn btn-success" href="../Keyword/edit/{{id}}"><i class="icon_pencil"></i></a>
                                                <a class="btn btn-danger" href="../Keyword/delete/{{id}}" onclick="return confirm('Are you sure you want to delete this item?');"><i class="icon_close_alt2"></i></a>
                                            </div>
                                        </td>
                                    </tr>
                                    {{/each}}
                                </tbody>
                            </table>
                        </section>
                    </div>
                </div>
        